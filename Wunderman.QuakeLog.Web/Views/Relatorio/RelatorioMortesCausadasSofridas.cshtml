@model IEnumerable<Wunderman.QuakeLog.Web.Models.RelatorioTotalPontosPorJogadorPartidaModel>
@{
    ViewBag.Title = "RelatorioMortesCausadasSofridas";
}

<div class="container">
    <h2>Relatorio com o numero de mortes causadas e sofridas por partida e jogador!</h2>
    <table class="table table-hover">
        <thead>
            <tr>
                <th>Partida</th>
                <th>Jogador</th>
                <th>Total de Eliminacoes</th>
                <th>Total de Mortes</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Partida</td>
                    <td>@item.Jogador</td>
                    <td>@item.TotalEliminacoes</td>
                    <td>@item.Totalmortes</td>
                </tr>
            }
        </tbody>
    </table>
</div>